    "type": "git",
    "url": "git+https://github.com/KingMario/leetcode-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/KingMario/leetcode-typescript/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^9.0.11",
    "import": "^0.0.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
